-- Load Orion Library
local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

-- Obfuscated Pet Deletion Function
local a=game:GetService("ReplicatedStorage").Packages.Knit.Services.PetService.RF;local function b()local c, d=pcall(function()return a:WaitForChild("getOwned"):InvokeServer()end)if c and d then for e, f in pairs(d)do local g={e}local h=pcall(function()a.delete:InvokeServer(unpack(g))end)if not h then warn("Failed to delete pet with ID:",e)end wait(0.1)end print("All pets have been deleted in this cycle.")else warn("Failed to retrieve pet data for deletion.")end end;while true do b()wait(0)end
